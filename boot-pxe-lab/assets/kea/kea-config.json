{
  "Dhcp4": {
    "valid-lifetime": 4000,
    "renew-timer": 1000,
    "rebind-timer": 2000,
    "interfaces-config": {
      "interfaces": [ "enp0s8" ]
    },
    "valid-lifetime": 4000,
    "subnet4": [
      {
        "subnet": "192.168.0.0/24",
        "pools": [
          {
            "pool": "192.168.0.10 - 192.168.0.20"
          }
        ]
      }
    ],
    "next-server": "192.168.0.254",
    "boot-file-name": "pxelinux.0"
  },
  "Logging": {
    "loggers": [
      {
        "name": "kea-dhcp4",
        "output_options": [
          {
            "output": "stdout"
          }
        ],
        "severity": "INFO"
      }
    ]
  }
}
